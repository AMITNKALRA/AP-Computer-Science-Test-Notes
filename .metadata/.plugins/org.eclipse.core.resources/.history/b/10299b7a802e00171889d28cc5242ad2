package VideoProj;

public class Human {
	
	private String name;
	private int age; // FORCE them to go thru methods. PUBLIC lets them change it
	
	public Human() { // has to be name of class
		
		name = "";
		age = 0;
		System.out.println("CONSTRUCTOR HERE");
		
	}
	
	public Human(String nm, int ag) {
		
		name = nm;
		age = a;
		
	}
	
	
}